{
  "name": "@checkdigit/eslint-config",
  "version": "4.1.0",
  "description": "Check Digit standard eslint configuration",
  "private": false,
  "main": "index.js",
  "prettier": "@checkdigit/prettier-config",
  "dependencies": {
    "@checkdigit/eslint-plugin": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecate": "^0.7.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-file-path-comment": "^1.0.2",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-sonarjs": "^0.7.0"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "scripts": {
    "prepublishOnly": "publish",
    "postpublish": "rimraf dist .npmignore",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "prettier": "prettier --list-different index.js src/**",
    "prettier:fix": "prettier --write index.js src/**",
    "test": "tsc --noEmit && eslint -f unix --ext .js,.ts src/** && npm run prettier"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/checkdigit/eslint-config.git"
  },
  "keywords": [],
  "author": "Check Digit, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/checkdigit/eslint-config/issues"
  },
  "homepage": "https://github.com/checkdigit/eslint-config#readme",
  "devDependencies": {
    "@checkdigit/prettier-config": "^1.0.2",
    "@checkdigit/publish": "^1.0.19",
    "@checkdigit/typescript-config": "2.1.0",
    "eslint": "^7.3.1",
    "prettier": "^2.3.0",
    "typescript": "^4.3.2"
  }
}
