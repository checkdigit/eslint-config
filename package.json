{
  "name": "@checkdigit/eslint-config",
  "version": "7.2.1",
  "description": "Check Digit standard eslint configuration",
  "private": false,
  "prettier": "@checkdigit/prettier-config",
  "main": "index.js",
  "engines": {
    "node": ">=16"
  },
  "peerDependencies": {
    "@checkdigit/eslint-plugin": "4.0.0",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-deprecate": "0.7.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-no-only-tests": "3.0.0",
    "eslint-plugin-no-secrets": "0.8.9",
    "eslint-plugin-sonarjs": "0.15.0",
    "eslint-plugin-unicorn": "43.0.2"
  },
  "scripts": {
    "postversion": "git push && git push --tags",
    "prettier": "prettier --list-different .",
    "prettier:fix": "prettier --write .",
    "lint": "eslint -f unix --ext .js,.ts src/**",
    "test": "npm run ci:compile && npm run ci:lint && npm run ci:style",
    "ci:compile": "tsc --noEmit",
    "ci:lint": "npm run lint",
    "ci:style": "npm run prettier"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/checkdigit/eslint-config.git"
  },
  "keywords": [
    "eslint"
  ],
  "author": "Check Digit, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/checkdigit/eslint-config/issues"
  },
  "homepage": "https://github.com/checkdigit/eslint-config#readme",
  "devDependencies": {
    "@checkdigit/prettier-config": "^3.0.0",
    "@checkdigit/typescript-config": "^3.1.0"
  }
}
