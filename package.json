{
  "name": "@checkdigit/eslint-config",
  "version": "11.4.1",
  "private": false,
  "description": "Check Digit standard eslint configuration",
  "keywords": [
    "eslint",
    "typescript"
  ],
  "homepage": "https://github.com/checkdigit/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/checkdigit/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/checkdigit/eslint-config.git"
  },
  "license": "MIT",
  "author": "Check Digit, LLC",
  "main": "index.mjs",
  "files": [
    "index.mjs"
  ],
  "scripts": {
    "ci:compile": "tsc --noEmit",
    "ci:lint": "npm run lint",
    "ci:style": "npm run prettier",
    "ci:test": "npm run ci:compile && npm run ci:lint && npm run ci:style && npm run test:node-mjs",
    "lint": "eslint --max-warnings 0 .",
    "lint:fix": "eslint --max-warnings 0 --fix .",
    "prepare": "",
    "prettier": "prettier --ignore-path .gitignore --list-different .",
    "prettier:fix": "prettier --ignore-path .gitignore --write .",
    "test": "npm run ci:test",
    "test:node-mjs": "node --disable-warning ExperimentalWarning --experimental-strip-types --test-timeout 600000 --test \"src/deprecated-rules.spec.ts\""
  },
  "prettier": "@checkdigit/prettier-config",
  "devDependencies": {
    "@checkdigit/prettier-config": "^7.1.1",
    "@checkdigit/typescript-config": "^9.3.0",
    "fclone": "^1.0.11"
  },
  "peerDependencies": {
    "@checkdigit/eslint-plugin": "7.17.0-PR.137-74ca",
    "@eslint/compat": "1.3.1",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.32.0",
    "@eslint/json": "0.13.1",
    "@eslint/markdown": "7.1.0",
    "eslint": "9.32.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-no-secrets": "2.2.1",
    "eslint-plugin-sonarjs": "3.0.4",
    "eslint-plugin-unicorn": "60.0.0",
    "eslint-plugin-yml": "1.18.0",
    "typescript-eslint": "8.39.0"
  },
  "engines": {
    "node": ">=22.15"
  }
}
