{
  "name": "@checkdigit/eslint-config",
  "version": "6.0.0",
  "description": "Check Digit standard eslint configuration",
  "private": false,
  "prettier": "@checkdigit/prettier-config",
  "main": "index.js",
  "engines": {
    "node": ">=16"
  },
  "peerDependencies": {
    "@checkdigit/eslint-plugin": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-deprecate": "^0.7.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-sonarjs": "^0.13.0"
  },
  "scripts": {
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "prettier": "prettier --list-different src/**",
    "prettier:fix": "prettier --write src/**",
    "test": "tsc --noEmit && eslint -f unix --ext .js,.ts src/** && npm run prettier"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/checkdigit/eslint-config.git"
  },
  "keywords": ["eslint"],
  "author": "Check Digit, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/checkdigit/eslint-config/issues"
  },
  "homepage": "https://github.com/checkdigit/eslint-config#readme",
  "devDependencies": {
    "@checkdigit/prettier-config": "^2.0.1",
    "@checkdigit/typescript-config": "3.0.0"
  }
}
