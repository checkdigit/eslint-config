{
  "name": "@checkdigit/eslint-config",
  "version": "8.3.0",
  "description": "Check Digit standard eslint configuration",
  "private": false,
  "prettier": "@checkdigit/prettier-config",
  "main": "index.js",
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "@checkdigit/eslint-plugin": "5.1.0",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-n": "16.0.2",
    "eslint-plugin-no-only-tests": "3.1.0",
    "eslint-plugin-no-secrets": "0.8.9",
    "eslint-plugin-sonarjs": "0.21.0",
    "eslint-plugin-unicorn": "48.0.1"
  },
  "scripts": {
    "prettier": "prettier --ignore-path .gitignore --list-different .",
    "prettier:fix": "prettier --ignore-path .gitignore --write .",
    "lint": "eslint --ignore-path .gitignore -f unix --ext .ts src index.js",
    "test": "npm run ci:test",
    "ci:test": "npm run ci:compile && npm run ci:lint && npm run ci:style",
    "ci:compile": "tsc --noEmit",
    "ci:lint": "npm run lint",
    "ci:style": "npm run prettier && npx eslint-config-prettier index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/checkdigit/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "typescript"
  ],
  "author": "Check Digit, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/checkdigit/eslint-config/issues"
  },
  "homepage": "https://github.com/checkdigit/eslint-config#readme",
  "devDependencies": {
    "@checkdigit/prettier-config": "^4.0.1",
    "@checkdigit/typescript-config": "4.1.0-PR.41-46cd",
    "@types/jest": "^29.5.4",
    "fclone": "^1.0.11",
    "jest": "^29.6.4",
    "ts-node": "^10.9.1"
  },
  "files": [
    "index.js"
  ]
}
